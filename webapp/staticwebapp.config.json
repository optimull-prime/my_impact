{
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ],
            "methods": [
                "GET",
                "POST",
                "OPTIONS"
            ]
        },
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/api/*"
        ]
    },
    "mimeTypes": {
        ".json": "application/json",
        ".wasm": "application/wasm"
    },
    "trailingSlash": "auto",
    "globalHeaders": {
        "Cache-Control": "max-age=3600"
    }
}